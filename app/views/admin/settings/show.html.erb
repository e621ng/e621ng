<div id="c-admin-settings">
  <div id="a-show">
    <h1>Settings</h1>
    <%= custom_form_for(:settings, url: admin_settings_path, method: :patch) do |f| %>
      <% Setting::GENERAL_SETTINGS.each do |key| %>
        <%= f.input key, **get_form_input_args(key) %>
      <% end %>
      <%= f.button :submit, value: "Submit" %>
    <% end %>
  </div>
</div>

<% content_for(:page_title) do %>
  Settings
<% end %>
