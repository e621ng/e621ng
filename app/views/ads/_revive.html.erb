      var s = document.createElement('script');
      s.async = true;
      s.src = "//<%= Danbooru.config.ads_config[:revive][:domain] %>/www/delivery/asyncjs.php";
      document.body.appendChild(s);
    })();
  <% end -%>
<% end %>
