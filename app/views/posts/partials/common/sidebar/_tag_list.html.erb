<%# locals: (category_name:, post:, tags:, query: ) %>
<% category_name = category_name.presence || "all" %>
<h5 class="tag-list-header <%= category_name %>-tag-list-header" data-category="<%= category_name %>"><%= TagCategory::HEADER_MAPPING[category_name] || "Tags" %></h5>
<ul class="tag-list <%= category_name %>-tag-list">
  <%= render partial: "/posts/partials/common/sidebar/tag_list_item", collection: tags, as: :tag, locals: { post: post, tag: tag, query: query } %>
</ul>
