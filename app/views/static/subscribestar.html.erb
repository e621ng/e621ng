<div id="c-static">
  <div id="a-subscribestar">

    <a href="<%= Danbooru.config.subscribestar_url %>" class="subscribestar-link">
      <%= image_tag("ss_logotype.png") %>
    </a>

    <div class="dtext-container">
      <%= format_text(@page.body, allow_color: true) %>
    </div>

    <a href="<%= Danbooru.config.subscribestar_url %>" class="subscribestar-support-us">
      Support on SubscribeStar
    </a>
  </div>
</div>

<% content_for(:secondary_links) do %>
  <% if CurrentUser.is_staff? %>
    <%= subnav_link_to "Wiki Page", show_or_new_wiki_pages_path(title: @page_name) %>
  <% end %>
<% end %>

<% content_for(:page_title) do %>
  SubscribeStar
<% end %>
