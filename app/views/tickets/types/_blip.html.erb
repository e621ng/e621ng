<% if @ticket.blip %>
    <tr>
      <td><span class='title'>Reported Blip</span></td>
      <td>
        <%= link_to "Blip by #{@ticket.blip.creator_name}", blip_path(@ticket.blip) %>
      </td>
    </tr>
<% else %>
    <tr>
      <td><span class='title'>Reported Blip</span></td>
      <td><span class='redtext'>Blip has been deleted</span></td>
    </tr>
<% end %>
